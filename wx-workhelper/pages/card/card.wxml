<view class="page-body">
  <view class="page-section page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 300px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      show-location
    >
    <cover-view class="image-cnt">
        <cover-image class="cover-image" bindtap="moveToLocation" src="/assets/image/position.png" />
      </cover-view>
    </map>
  </view>
  </view>